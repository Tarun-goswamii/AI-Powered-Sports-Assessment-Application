{
  "version": 2,
  "builds": [
    {
      "src": "api/convex/[functionName].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/convex/(.*)",
      "dest": "/api/convex/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "env": {
    "FLUTTER_WEB": "true"
  }
}