{
  "name": "sports-assessment-backend",
  "version": "1.0.0",
  "description": "Backend for AI Sports Talent Assessment App",
  "main": "index.js",
  "scripts": {
    "build": "flutter build web --release",
    "deploy": "convex deploy",
    "dev": "convex dev",
    "deploy-and-run": "npm run deploy && flutter run",
    "start": "start-complete.bat",
    "start-complete": "start-complete.bat",
    "start-with-ml": "start-complete.bat",
    "start-global": "start-with-ngrok.bat",
    "setup": "convex deploy && flutter pub get",
    "setup-ml": "pip install -r requirements.txt && npm run setup",
    "ml-server": "python python_ml_server.py",
    "seed-data": "npx convex run seed_data:seedAllData",
    "clear-data": "npx convex run seed_data:clearAllData",
    "seed-mentors": "npx convex run functions_additional:seedDemoMentors",
    "clear-mentors": "npx convex run functions_additional:clearAllMentors",
    "seed-demo": "npx convex run functions_additional:quickSeedDemo",
    "seed-full-demo": "npx convex run functions_additional:seedComprehensiveDemoData",
    "clear-demo": "npx convex run functions_additional:clearAllDemoData",
    "setup-demo": "node scripts/seed-all-demo-data.js",
    "watch": "convex dev --watch",
    "deploy-ml": "deploy-alternatives.bat",
    "deploy-render": "deploy-render.bat",
    "build-prod": "build-production.bat",
    "deploy-railway": "deploy-railway.bat",
    "vercel-build": "flutter build web --release",
    "vercel-dev": "flutter run -d web-server --web-port=3000",
    "vercel-deploy": "vercel --prod",
    "vercel-preview": "vercel"
  },
  "dependencies": {
    "convex": "^1.27.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "convex",
    "backend",
    "sports"
  ],
  "author": "Sports Assessment Team",
  "license": "ISC"
}
